@using ESEIM.Utils
@inject Microsoft.AspNetCore.Http.IHttpContextAccessor HttpContextAccessor
@{
    ViewData["Title"] = "Quản trị liên hệ";
    Layout = "~/Areas/Admin/Views/Shared/_Layout_Admin.cshtml";
    var session = HttpContextAccessor.HttpContext.GetSessionUser();
    bool hasPermissionContactSupplierInsert = session.HasPermission(CommonUtil.Controller_ContactSupplier, CommonUtil.Action_Insert);
    bool hasPermissionContactSupplierUpdate = session.HasPermission(CommonUtil.Controller_ContactSupplier, CommonUtil.Action_Update);
    bool hasPermissionContactSupplierDelete = session.HasPermission(CommonUtil.Controller_ContactSupplier, CommonUtil.Action_Delete);
}
<div id="contentMain" ng-app="App_ESEIM" ng-controller="Ctrl_ESEIM">
    <div ng-view></div>
</div>
<script>
    var PERMISSION_CONTACT_SUPPLIER = {
        Insert: @(hasPermissionContactSupplierInsert ? "true" : "false"),
        Update: @(hasPermissionContactSupplierUpdate ? "true" : "false"),
        Delete: @(hasPermissionContactSupplierDelete ? "true" : "false"),
    };
</script>
<script src="~/views/admin/contactSupplier/controller.js" type="text/javascript"></script>