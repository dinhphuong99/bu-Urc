<script>
    var PERMISSION_MATERIAL_PRODUCT = {
        Insert: @(hasPermissionMaterialProductsInsert ? "true" : "false"),
        Update: @(hasPermissionMaterialProductsUpdate ? "true" : "false"),
        Delete: @(hasPermissionMaterialProductDelete ? "true" : "false"),
    };
      var PERMISSION_MATERIAL_PRODUCT_GROUP = {
        Insert: @(hasPermissionMaterialProductsGroupInsert ? "true" : "false"),
        Update: @(hasPermissionMaterialProductsGroupUpdate ? "true" : "false"),
        Delete: @(hasPermissionMaterialProductsGroupDelete ? "true" : "false"),
    };
     var PERMISSION_SERVICE_CATEGORY_PRICE = {
        Insert: @(hasPermissioServiceCategoryPriceInsert ? "true" : "false"),
        Update: @(hasPermissioServiceCategoryPriceUpdate ? "true" : "false"),
        Delete: @(hasPermissionServiceCategoryPriceDelete ? "true" : "false"),
    };
</script>
