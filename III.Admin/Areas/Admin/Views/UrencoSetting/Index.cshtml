@using ESEIM.Utils
@inject Microsoft.AspNetCore.Http.IHttpContextAccessor HttpContextAccessor
@{
    ViewData["Title"] = "Quản lý cấu hình tham số";
    Layout = "~/Areas/Admin/Views/Shared/_Layout_Admin.cshtml";
    var session = HttpContextAccessor.HttpContext.GetSessionUser();
    bool hasPermissionMaterialProductsInsert = session.HasPermission(CommonUtil.Controller_MaterialProduct, CommonUtil.Action_Insert);
    bool hasPermissionMaterialProductsUpdate = session.HasPermission(CommonUtil.Controller_MaterialProduct, CommonUtil.Action_Update);
    bool hasPermissionMaterialProductDelete = session.HasPermission(CommonUtil.Controller_MaterialProduct, CommonUtil.Action_Delete);
    bool hasPermissionMaterialProductsGroupInsert = session.HasPermission(CommonUtil.Controller_MaterialProductGroup, CommonUtil.Action_Insert);
    bool hasPermissionMaterialProductsGroupUpdate = session.HasPermission(CommonUtil.Controller_MaterialProductGroup, CommonUtil.Action_Update);
    bool hasPermissionMaterialProductsGroupDelete = session.HasPermission(CommonUtil.Controller_MaterialProductGroup, CommonUtil.Action_Delete);
    bool hasPermissioServiceCategoryPriceInsert = session.HasPermission(CommonUtil.Controller_ServiceCategoryPrice, CommonUtil.Action_Insert);
    bool hasPermissioServiceCategoryPriceUpdate = session.HasPermission(CommonUtil.Controller_ServiceCategoryPrice, CommonUtil.Action_Update);
    bool hasPermissionServiceCategoryPriceDelete = session.HasPermission(CommonUtil.Controller_ServiceCategoryPrice, CommonUtil.Action_Delete);
}
@section plugin_script {
    <script src="~/lib/assets/global/plugins/angulerjs-taginput/ng-tags-input.min.js"></script>
    <script src="~/lib/assets/global/plugins/tinymce/tinymce.min.js"></script>
    <script src="~/lib/assets/global/plugins/tinymce/config.tinymce.js"></script>
}
@section plugin_style{
    <link href="~/lib/assets/global/plugins/angulerjs-taginput/ng-tags-input.min.css" rel="stylesheet" />
}
